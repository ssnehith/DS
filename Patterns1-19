static void pat16(int n){
    int sp;
    int st;
        for(int r=1;r<=n;r++){
            st=2*r-1;
            sp=n-r;
            for(int c=1;c<=sp;c++){
                	System.out.print(" ");
            }
            for(int c=1;c<=st;c++){
                if(c==1||c==st){
                	System.out.print("1");
            }
            else if(r%2==0&&c%2==0&&c<st){
                 System.out.print(r-1);
             }
            else{
                System.out.print(" ");
            }
            /*if(c%2==1&&c<st){
                 System.out.print(c-1);
             }*/
            }
            	System.out.println();
        }
}

static void pat17(int n){
        for(int r=1;r<=n;r++){
            for(int c=1;c<=n-r;c++){
                	System.out.print(" ");
            }
            for(int c=r;c>=1;c--){
               System.out.print(c); 
            }
            for(int c=2;c<=r;c++){
               System.out.print(c); 
            }
            	System.out.println();
        }
}
static void pat18(int n){
        for(int r=1;r<=2*n;r++){
            if(r==1||r==2*n){
            for(int c=1;c<=2*n;c++){
           	System.out.print("*");
            }
            }
            else if(r<=n){
             for(int c=1;c<=2*n;c++){
            if(c<=n-r+1){
            System.out.print("*");   
            }
            else if(c>n-r+1&&c<=2*n-(n-r+1)){
            System.out.print(" ");   
            }
            else if(c>2*n-(n-r+1)&&c<=2*n){
            System.out.print("*"); 
            }
             }
            }
            else if(r>n){
               for(int c=1;c<=2*n;c++){
                 if(c<=r-n){
                 System.out.print("*");
                 }
                 else if(c>r-n&&c<=2*n-(r-n)){
                  System.out.print(" ");   
                 }
                 else{
                    System.out.print("*"); 
                 }
                 }  
               } 
            	System.out.println();
        }
}
		static void pat19(int n){
        for(int r=1;r<=2*n-1;r++){
             if(r<=n){
             for(int c=1;c<=2*n;c++){
                 if(r==n){
            System.out.print("*");    
            }
            else if(c<=r){
            System.out.print("*");   
            }
            else if(c>r&&c<=2*n-r){
            System.out.print(" ");   
            }
            else{
            System.out.print("*"); 
            }
             }
            }
            else if(r>n){
               for(int c=1;c<=2*n;c++){
                 if(c<=(2*n-r)){
                 System.out.print("*");
                 }
                 else if(c>2*n-r&&c<=r){
                  System.out.print(" ");   
                 }
                 else{
                    System.out.print("*"); 
                 }
                 }  
               } 
            	System.out.println();
        }
}	
}
